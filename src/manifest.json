{
  "manifest_version": 3,
  "name": "JSON Formatter",
  "description": "The best JSON Formatter!",
  "version": "0.2.1",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png"
      ],
      "matches": []
    },
    {
      "resources": [
        "*.svg"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}